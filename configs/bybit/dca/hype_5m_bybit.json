{
  "strategy": {
    "symbol": "HYPEUSDT",
    "category": "linear",
    "data_file": "data\\bybit\\linear\\HYPEUSDT\\5\\candles.csv",
    "base_amount": 40,
    "max_multiplier": 3.5,
    "price_threshold": 0.01,
    "price_threshold_multiplier": 1.25,
    "interval": "5m",
    "window_size": 100,
    "tp_percent": 0.03,
    "use_tp_levels": true,
    "auto_tp_orders": true,
    "cycle": true,
    "indicators": ["hull_ma", "stochastic_rsi", "macd", "obv"],
    "macd": {
      "fast_period": 6,
      "slow_period": 26,
      "signal_period": 9
    },
    "hull_ma": {
      "period": 25
    },
    "obv": {
      "trend_threshold": 0.005
    },
    "stochastic_rsi": {
      "period": 12,
      "overbought": 90,
      "oversold": 20
    }
  },
  "exchange": {
    "name": "bybit",
    "bybit": {
      "api_key": "${BYBIT_API_KEY}",
      "api_secret": "${BYBIT_API_SECRET}",
      "testnet": false,
      "demo": true
    }
  },
  "risk": {
    "initial_balance": 500,
    "commission": 0.0005,
    "min_order_qty": 0.01
  },
  "notifications": {
    "enabled": false,
    "telegram_token": "${TELEGRAM_TOKEN}",
    "telegram_chat": "${TELEGRAM_CHAT_ID}"
  }
}
