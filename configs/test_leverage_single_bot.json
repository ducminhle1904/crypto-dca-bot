{
  "strategy": {
    "symbol": "BTCUSDT",
    "category": "linear",
    "base_amount": 40.0,
    "max_multiplier": 2.0,
    "leverage": 10.0,
    "allocation_percentage": 1.0,
    "max_position_size": 400.0,
    "interval": "5m",
    "window_size": 100,
    "tp_percent": 0.02,
    "use_tp_levels": true,
    "auto_tp_orders": true,
    "tp_levels": 5,
    "tp_quantity": 0.2,
    "cycle": true,
    "indicators": ["hull_ma", "stochastic_rsi", "keltner"],
    "dca_spacing": {
      "strategy": "fixed",
      "parameters": {
        "base_threshold": 0.02,
        "threshold_multiplier": 1.1,
        "max_threshold": 0.08,
        "min_threshold": 0.005
      }
    },
    "hull_ma": {
      "period": 20
    },
    "stochastic_rsi": {
      "period": 14,
      "overbought": 80.0,
      "oversold": 20.0
    },
    "keltner_channels": {
      "period": 20,
      "multiplier": 2.0
    }
  },
  "exchange": {
    "name": "bybit",
    "bybit": {
      "api_key": "${BYBIT_API_KEY}",
      "api_secret": "${BYBIT_API_SECRET}",
      "testnet": false,
      "demo": true
    }
  },
  "risk": {
    "initial_balance": 500.0,
    "commission": 0.0005
  },
  "notifications": {
    "enabled": false
  }
}
